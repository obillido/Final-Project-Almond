<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
  PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
  "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="project.mybatis.mapper.TicketMapper">
	<select id="getInfoList" parameterType="int" resultType="project.app.almond.vo.TicketVo">
		select * from ticket where cultype=#{cultype}
	</select>
	<select id="getinfo" parameterType="int" resultType="project.app.almond.vo.TicketVo">
		select * from ticket where tknum=#{tknum}
	</select>
</mapper>





